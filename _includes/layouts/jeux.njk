---
layout: layouts/base.njk
section: jeux
permalink: /jeux/index.html
---
<h1>{{ title }}</h1>

{{ layoutContent | safe }}
{% set postslist = collections.jeu %}
<section>
	{% for post in postslist | reverse %}
		<article{% if post.url == url %} data-current="current item"{% endif %}>
			<h3>
				<a href="{{ post.url | url }}">
					{% if post.data.title %}
						{{ post.data.title }}
					{% else %}
						Untitled
					{% endif %}
				</a>
			</h3>
		</article>
	{% endfor %}
</section>


